{
  "name": "observer",
  "version": "1.0.0",
  "description": "Observer api for logging astronomical observations",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "start:local": "cross-env NODE_ENV=local nodemon --watch './src/**/*.ts' --exec ts-node ./src/index.ts",
    "migrate:create": "sequelize db:migrate --config ./src/db/config/config.js --migrations-path ./src/db/migrations",
    "migrate:reset": "sequelize db:migrate:undo",
    "seed:create": "npx sequelize-cli seed:generate --name reports.seed",
    "seed:run": "sequelize db:seed:all --config ./src/db/config/config.js --seeders-path ./src/db/seeders"
  },
  "author": "Nazar Panchenko",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "mysql2": "^3.1.2",
    "sequelize": "^6.29.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "sequelize-cli": "^6.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "winston": "^3.8.2"
  }
}
